                        a.innerHTML = '<i class="far fa-heart fas" style="pointer-events: none;"></i>Remove From Wishlist'
                    }
                }
                a.addEventListener('click', wishlistHandler);
            })
        }

        function cartHandler(e) {
            const obj = {
                id: e.target.dataset.id,
                image: e.target.dataset.image,
                title: e.target.dataset.title,
                price: e.target.dataset.price,
                quantity: 1
            }
            if (user_id) {
                $.post('/api/account/cart', {
                    user_id: user_id,
                    pro_id: e.target.dataset.id,
                    quantity: obj.quantity
                }, (data, status) => {
                    if (status === 'success') {
                        console.log('success');
                        let cart = window.localStorage.getItem('user_cart');
                        if (cart) {
                            cart = JSON.parse(cart);
                            if (cart.some(e => e.id === obj.id)) {
                                cart.filter(e => e.id === obj.id)[0].quantity++;
                            }
                            else {
                                cart.push(obj)
                            }
                        }
                        else {
                            cart = [obj];
                        }
                        window.localStorage.setItem('user_cart', JSON.stringify(cart));
                        updateCartBar();
                    }
                    else {
                        console.log(data);
                    }
                })
            }
            else {

                let cart = window.localStorage.getItem('cart');
                if (cart) {
                    cart = JSON.parse(cart);
                    if (cart.some(e => e.id === obj.id)) {
                        cart.filter(e => e.id === obj.id)[0].quantity++;
                    }
                    else {
                        cart.push(obj)
                    }
                }
                else {
                    cart = [obj];
                }
                window.localStorage.setItem('cart', JSON.stringify(cart));
            }
            e.target.textContent = "Add one more";
            updateCartBar();
        }
        function add_cart_event() {
            let cart = window.localStorage.getItem('cart');
            if (cart && cart.length > 2) {

            }
            else {
                cart = window.localStorage.getItem('user_cart');
            }
            document.querySelectorAll('.cart').forEach(a => {
                if (cart.includes(a.dataset.id)) {
                    a.textContent = "Add one more"
                }
                a.addEventListener('click', cartHandler);
            })
        }
        function updateCartBar() {
            let cart = window.localStorage.getItem('cart');
            if (cart && cart.length > 2) {

            }
            else {
                cart = window.localStorage.getItem('user_cart');
            }
            if (!cart) {
                return;
            }
            cart = JSON.parse(cart);
            const cart_box = document.querySelector('#cart-bar')
            cart_box.innerHTML = '';
            let total = 0;
            cart.forEach(item => {
                const li = `<li>
                            <a href="./product/${item.id}" class="photo"><img src="${item.image}" class="cart-thumb" alt="" /></a>
                            <h6><a href="./product/${item.id}">${item.title} </a></h6>
                            <p>${item.quantity}x - <span class="price">$${item.price}</span></p>
                        </li>`;
                total += Math.round(parseFloat(item.price) * parseInt(item.quantity) * 100, 2) / 100;
                cart_box.innerHTML += li;
            })
            total = Math.round(total * 100) / 100;
            document.querySelector('#cart-total').textContent = total;
            document.querySelector('#cart-total-item').textContent = cart.length;
        }
        updateCartBar();

