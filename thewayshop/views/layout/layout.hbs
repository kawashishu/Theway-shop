    <script>
        const user_id = '{{{user.id}}}';
        if (user_id) {
            $.get(`/api/account/${user_id}/wishlist`, (data, status) => {
                if (status === 'success') {
                    const wishList = window.localStorage.getItem('wishList');
                    if (wishList && wishList.length > 2) {
                        const list_item = JSON.parse(wishList);
                        list_item.forEach(item => {
                            if (!data.some(i => i.id === item.id)) {
                                $.post('/api/account/wishlist', {
                                    user_id: user_id,
                                    pro_id: item.id
                                }, (d, status) => {
                                    if (status === 'success') {
                                        data.push(item);
                                    }
                                })
                            }
                        })
                    }
                    window.localStorage.setItem('wishList', '[]');
                    window.localStorage.setItem('user_wishlist', JSON.stringify(data));
                }
            })
            $.get(`/api/account/${user_id}/cart`, (data, status) => {
                if (status === 'success') {
                    const cart = window.localStorage.getItem('cart');
                    if (cart && cart.length > 2) {
                        const list_item = JSON.parse(cart);
                        list_item.forEach(item => {
                            if (!data.some(i => i.id === item.id)) {
                                $.post('/api/account/cart', {
                                    user_id: user_id,
                                    pro_id: item.id,
                                    quantity: item.quantity
                                }, (d, status) => {
                                    if (status === 'success') {
                                        data.push(item);
                                    }
                                })
                            }
                        })
                    }
                    window.localStorage.setItem('cart', '[]');
                    window.localStorage.setItem('user_cart', JSON.stringify(data));
                    updateCartBar();
                }
            })

        }
        else {
            window.localStorage.setItem('user_wishlist', '');
            window.localStorage.setItem('user_cart', '');
