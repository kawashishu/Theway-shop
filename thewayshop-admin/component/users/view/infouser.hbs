
<!--main content start-->
<section id="main-content">
    <section class="wrapper">
        <!-- BASIC FORM ELELEMNTS -->
        <div class="row mt">
            <div class="col-lg-12">
                <div class="form-panel">
                    <h4 class="mb"><i class="fa fa-angle-right"></i> {{manager.fullname}}</h4>
                    <div class="row">
                        <div class="col-lg-4">
                            <img {{#if this.image}}src="{{this.image}}"{{else}}src="/images/friends/fr-06.jpg"{{/if}} class="img-circle" width="80">
                        </div>
                    </div>
                    <form class="form-horizontal style-form" method="post">
                            {{#each record}}
                             <div class="form-group">
                            <label class="col-sm-2 col-sm-2 control-label">{{@key}}</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" name="{{@key}}" value="{{this}}">
                            </div>
                            {{#ifeq @key 'image'}}
                            <img src="{{this}}" width="80">
                            {{/ifeq}}
                        </div>
                        {{/each}}
                           <button type = "button" onclick="back()">Ok</button>           
                    </form>
                </div>
            </div><!-- col-lg-12-->      	
        </div><!-- /row -->
    </section><! --/wrapper -->
</section><!-- /MAIN CONTENT -->
 <script>
      function back(){
          history.back();
      }
  </script>
<!--main content end-->
      